{
  "$includes": ["_variables"],
  "output": "blocks/server_rack/front.error",
  "layers": {
    "base": { "texture": "blocks/server_rack/front.base.png" },
    "on.light": {
      "texture": "blocks/server_rack/front.mask.on.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "error_color" }
      }
    },
    "anim.0": {
      "texture": "blocks/server_rack/front.mask.cut0.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "error_color" }
      }
    },
    "anim.1": {
      "texture": "blocks/server_rack/front.mask.cut1.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "error_color" }
      }
    },
    "anim.2": {
      "texture": "blocks/server_rack/front.mask.cut2.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "error_color" }
      }
    }
  },
  "frames": [
    {
      "layers": ["base", "on.light", "anim.0", "anim.1", "anim.2"]
    }
  ]
}