{
  "$includes": ["_variables"],
  "output": "blocks/server_controller/front.error",
  "layers": {
    "base": { "texture": "blocks/server_controller/front.png" },
    "on.light": {
      "texture": "blocks/server_controller/front.mask.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "error_color" }
      }
    },
    "anim.0": {
      "texture": "blocks/server_controller/front.mask2/0.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "error_color" }
      }
    },
    "anim.1": {
      "texture": "blocks/server_controller/front.mask2/1.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "error_color" }
      }
    },
    "anim.2": {
      "texture": "blocks/server_controller/front.mask2/2.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "error_color" }
      }
    },
    "anim.3": {
      "texture": "blocks/server_controller/front.mask2/3.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "error_color" }
      }
    }
  },
  "frames": [
    {
      "layers": ["base", "on.light", "anim.0", "anim.1", "anim.2", "anim.3"]
    }
  ]
}