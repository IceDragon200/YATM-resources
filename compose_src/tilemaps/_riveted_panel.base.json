    "highlight.signal": {
      "texture": "tilemaps/riveted_panel.mask.t4x3.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "signal_color" }
      }
    },
    "highlight.error": {
      "texture": "tilemaps/riveted_panel.mask.t4x3.png",
      "blend": {
        "type": "multiply_color",
