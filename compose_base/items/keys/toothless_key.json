{
  "$includes": ["_variables"],
  "output": "items/key/$material$_$color$_toothless",
  "layers": {
    "base": { "texture": "items/keys/toothless_$material$.png" },
    "banner": {
      "texture": "items/keys/band.mask.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "dye.$color$" }
      }
    }
  },
  "frames": [
    { "layers": ["base", "banner"] }
  ]
}
