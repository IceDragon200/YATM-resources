{
  "$includes": ["_variables", "_dye"],
  "output": "gui/colored_rotary_buttons/$color$.3",
  "layers": {
    "base": {
      "texture": "gui/button.base.rotary.48px.png"
    },
    "mask": {
      "texture": "gui/button.rotary.3.48px.png",
      "blend": {
        "type": "multiply_color",
        "value": { "$variable": "dye.$color$" }
      }
    }
  },
  "frames": [
    { "layers": ["base", "mask"] }
  ]
}